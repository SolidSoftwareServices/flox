@using Ei.Rp.DomainModels.MappingValues

@model EI.RP.WebApp.Flows.AppFlows.Accounts.Components.AccountCardBillingDetails.ViewModel

@if (Model.AccountType == ClientAccountType.EnergyService)
{
    @await Html.PartialAsync("./Partials/_EnergyServiceBillingDetails.cshtml", Model)
}
else if (Model.IsEqualizer)
{
    @await Html.PartialAsync("./Partials/_EqualizerBillingDetails.cshtml", Model)
}
else
{
    @await Html.PartialAsync("./Partials/_DefaultBillingDetails.cshtml", Model)
}
